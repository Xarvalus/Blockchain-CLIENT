[ignore]
.*/node_modules/.*
.*/internals/.*
.*/server/.*
.*/coverage/.*
.*/.next/.*

[include]

[libs]
flow-typed

[lints]

[options]
all=true
emoji=true
esproposal.decorators=ignore

module.name_mapper='^components\/(.*)$' -> '<PROJECT_ROOT>/app/components/\1'
module.name_mapper='^containers\/(.*)$' -> '<PROJECT_ROOT>/app/containers/\1'

[strict]
